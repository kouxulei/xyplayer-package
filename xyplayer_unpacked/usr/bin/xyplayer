#!/usr/bin/python3

import sys
from PyQt5.QtWidgets import QApplication

def main():
    app = QApplication(sys.argv)
    app.setApplicationName("xyplayer")
    app.setQuitOnLastWindowClosed(True)
    
    from xyplayer.player import Player
    Player = Player()
    Player.show()
    sys.exit(app.exec_())

if __name__ == '__main__':
    main()
